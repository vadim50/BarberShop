<table class="table table-striped">
<thead>
	<tr>
		<th>#</th>
		<th>Имя</th>
		<th>Телефон</th>
		<th>Дата посещения</th>
		<th>Мастер</th>
		<th>Цвет</th>
	</tr>
</thead>
<tbody>
<% @results.each do |row| %>
	<tr>
		<th scope="row"><%= row['id'] %></th>
		<td><%= row['username'] %></td>
		<td><%= row['phone']%></td>
		<td><%= row['datestamp'] %></td>
		<td><%= row['barber'] %></td>
		<td><%= row['color'] %></td>
	</tr>
<% end %>
</tbody>
</table>

